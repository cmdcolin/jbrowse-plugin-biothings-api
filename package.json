{
  "name": "jbrowse-plugin-biothings",
  "version": "1.0.5",
  "description": "JBrowse 2 biothings API plugin",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "config": {
    "port": 9000,
    "browse": {
      "port": 8999
    },
    "jbrowse": {
      "plugin": {
        "name": "Biothings"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all --sequential clean --parallel start:*",
    "start:watch": "cross-env JB_NPM=false rollup --config --watch --bundleConfigAsCjs",
    "start:server": "serve --cors --listen $npm_package_config_port .",
    "prebuild": "npm-run-all clean",
    "build": "rollup --config --bundleConfigAsCjs",
    "postbuild": "cp distconfig.json dist/config.json",
    "browse": "npm-run-all jbrowse:*",
    "jbrowse:configure": "shx cp jbrowse_config.json .jbrowse/config.json",
    "jbrowse:serve": "cross-var serve --listen $npm_package_config_browse_port .jbrowse",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx src/",
    "prepack": "npm-run-all build",
    "postversion": "git push --follow-tags"
  },
  "jbrowse-plugin": {
    "name": "Biothings"
  },
  "dependencies": {
    "string-template": "^1.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-react": "^7.10.4",
    "@emotion/react": "^11.10.4",
    "@jbrowse/cli": "^2.6.1",
    "@jbrowse/core": "^2.6.1",
    "@jbrowse/development-tools": "^2.1.1",
    "@mui/material": "^5.10.5",
    "@mui/system": "^5.10.5",
    "@mui/x-data-grid": "^6.9.2",
    "@schemastore/package": "^0.0.9",
    "@types/node": "^16.11.13",
    "@types/react": "^17.0.0",
    "@types/string-template": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-eslint": "^10.0.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "cypress": "^12.8.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.0.8",
    "mobx": "^6.0.0",
    "mobx-react": "^7.0.0",
    "mobx-state-tree": "5.1.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.0.0",
    "rxjs": "^7.8.1",
    "serve": "^14.2.0",
    "shx": "^0.3.3",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.3.0",
    "tss-react": "^4.8.6",
    "typescript": "^5.1.6"
  }
}
